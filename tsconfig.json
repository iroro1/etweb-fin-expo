{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "expo-router/entry"
    ]
  },
  "include": [
    "src",
    "app.config.ts",
    "expo-env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
